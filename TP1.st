Object subclass: #AlgoRemis	instanceVariableNames: 'choferes viajes'	classVariableNames: ''	package: 'TP1'!!AlgoRemis methodsFor: 'initialization' stamp: 'gonza 10/7/2021 15:59'!initialize 	choferes := OrderedCollection new.	viajes := OrderedCollection new.! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'gonza 10/8/2021 16:03'!registrarChoferDeAutoElectrico: nombreChofer conTarifaInicial:  unaTarifa|chofer|(unaTarifa<0)ifTrue:[		^ TarifaInicialNegativa  signal.		].chofer := Chofer nuevoChoferAutoElectrico: nombreChofer conTarifaInicial: unaTarifa.choferes add: chofer.! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'gonza 10/14/2021 15:28'!viajeMasBaratoParaDestino: unDestino|viajeAEseDestino |viajeAEseDestino := viajes detect:  [ :v | 	(v obtenerDestino ) = unDestino.	 ] ifNone: [^ DestinoNoEncontrado signal].viajeAEseDestino viajeMasBaratoSegunChoferes: choferes.^ viajeAEseDestino .! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'gonza 10/8/2021 16:03'!registrarChoferDeAuto: nombreChofer conTarifaInicial:  unaTarifa|chofer|(unaTarifa<0)ifTrue:[		^ TarifaInicialNegativa  signal.		].chofer := Chofer nuevoChoferAutoRegular: nombreChofer conTarifaInicial: unaTarifa.choferes add: chofer.! !!AlgoRemis methodsFor: 'accessing' stamp: 'gonza 10/14/2021 14:33'!crearViajeConDestino: nombreDestino aKms: cantKms peajes: cantPeajes |viaje| (cantKms<0 )ifTrue:[		^ CantKmsNegativa   signal.		].(cantPeajes <0 )ifTrue:[		^ CantPeajesNegativa   signal.		].(nombreDestino includesSubstring: 'Hospital') ifTrue: [	 viaje :=  Viaje nuevoViajeConDestinoHospital: nombreDestino aKms: cantKms peajes: cantPeajes. 	]ifFalse: [		viaje :=  Viaje nuevoViajeConDestino: nombreDestino aKms: cantKms peajes: cantPeajes. 		].viajes add: viaje.! !Object subclass: #AutoElectrico	instanceVariableNames: 'precioporkm preciopeaje'	classVariableNames: ''	package: 'TP1'!!AutoElectrico methodsFor: 'initialization' stamp: 'gonza 10/3/2021 17:19'!initialize precioporkm:= 30.preciopeaje:= 50.! !!AutoElectrico methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 19:29'!presupuestoPorKM: cantKms Peajes: cantPeajes^(( preciopeaje * cantPeajes)  + (precioporkm  * cantKms ))! !Object subclass: #AutoRegular	instanceVariableNames: 'precioporkm preciopeaje'	classVariableNames: ''	package: 'TP1'!!AutoRegular methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 19:31'!presupuestoPorKM: cantKms Peajes: cantPeajes^(( preciopeaje * cantPeajes)  + (precioporkm  * cantKms ))! !!AutoRegular methodsFor: 'initialization' stamp: 'gonza 10/3/2021 17:20'!initialize precioporkm:= 50.preciopeaje := 100.! !Error subclass: #CantKmsNegativa	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #CantPeajesNegativa	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Chofer	instanceVariableNames: 'nombre tarifaInicial auto'	classVariableNames: ''	package: 'TP1'!!Chofer methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 17:09'!obtenerNombre^nombre.! !!Chofer methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 19:32'!presupuestoPorKM: cantKms Peajes: cantPeajes^  (tarifaInicial + (auto presupuestoPorKM: cantKms Peajes: cantPeajes)).! !!Chofer methodsFor: 'accessing' stamp: 'gonza 10/3/2021 16:37'!tarifa: unaTarifa tarifaInicial   := unaTarifa  .! !!Chofer methodsFor: 'accessing' stamp: 'gonza 10/3/2021 17:07'!auto: unTipoDeAutoauto:= unTipoDeAuto.! !!Chofer methodsFor: 'accessing' stamp: 'gonza 10/3/2021 16:37'!nombre: unNombrenombre  := unNombre .! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Chofer class	instanceVariableNames: ''!!Chofer class methodsFor: 'as yet unclassified' stamp: 'gonza 10/6/2021 18:07'!nuevoChoferAutoElectrico: nombreChofer conTarifaInicial: unaTarifa|chofer|chofer := self new.chofer nombre: nombreChofer.chofer tarifa: unaTarifa .chofer auto: AutoElectrico new.^chofer.! !!Chofer class methodsFor: 'as yet unclassified' stamp: 'gonza 10/3/2021 17:07'!nuevoChoferAutoRegular: nombreChofer conTarifaInicial: unaTarifa|chofer|chofer := self new.chofer nombre: nombreChofer.chofer tarifa: unaTarifa .chofer auto: AutoRegular new.^chofer.! !Object subclass: #DescuentoHospital	instanceVariableNames: 'bonificacion'	classVariableNames: ''	package: 'TP1'!!DescuentoHospital methodsFor: 'initialization' stamp: 'gonza 10/7/2021 16:38'!initializebonificacion:= 0.2! !!DescuentoHospital methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 17:07'!bonificacion^bonificacion .! !Error subclass: #DestinoNoEncontrado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #SinDescuento	instanceVariableNames: 'bonificacion'	classVariableNames: ''	package: 'TP1'!!SinDescuento methodsFor: 'as yet unclassified' stamp: 'gonza 10/13/2021 17:07'!bonificacion^bonificacion .! !!SinDescuento methodsFor: 'initialization' stamp: 'gonza 10/7/2021 16:38'!initializebonificacion:= 1! !Error subclass: #TarifaInicialNegativa	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Viaje	instanceVariableNames: 'descuento chofer destino kms peajes'	classVariableNames: ''	package: 'TP1'!!Viaje methodsFor: 'as yet unclassified' stamp: 'gonza 10/7/2021 16:00'!obtenerDestino ^destino! !!Viaje methodsFor: 'as yet unclassified' stamp: 'gonza 10/14/2021 15:52'!precio^((descuento bonificacion) * (chofer presupuestoPorKM: kms Peajes: peajes) )! !!Viaje methodsFor: 'as yet unclassified' stamp: 'gonza 10/14/2021 15:39'!viajeMasBaratoSegunChoferes: choferes|choferMasBarato|choferMasBarato := (choferes detectMin:  [:c | c  presupuestoPorKM: kms  Peajes: peajes  ]).self asignarChofer: choferMasBarato .! !!Viaje methodsFor: 'as yet unclassified' stamp: 'gonza 10/14/2021 15:31'!asignarChofer: choferMasBaratochofe